<UserControl x:Class="PokerClient.GUI.MainMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PokerClient.GUI"
             mc:Ignorable="d" 
             d:DesignHeight="640" d:DesignWidth="800">
    <Grid>
        <DockPanel LastChildFill="True">
            <ContentControl x:Name="topMainPanel" DockPanel.Dock="Top" Height="20" Background="Black"/>
            <ContentControl x:Name="bottomMainPanel" DockPanel.Dock="Bottom" >
            </ContentControl>
        </DockPanel>

    </Grid>
</UserControl>
